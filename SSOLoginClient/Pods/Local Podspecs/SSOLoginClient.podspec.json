{
  "name": "SSOLoginClient",
  "version": "0.0.1",
  "license": "Apache License, Version 2.0",
  "authors": {
    "jiangzhenjie": "zhenjie_jiang@foxmail.com"
  },
  "homepage": "https://github.com/jiangzhenjie/SSOLoginDemo",
  "summary": "SSOLoginDemo base on gRPC",
  "source": {
    "git": "https://github.com/jiangzhenjie/SSOLoginDemo"
  },
  "platforms": {
    "ios": "7.1",
    "osx": "10.9"
  },
  "dependencies": {
    "!ProtoCompiler-gRPCPlugin": [
      "~> 1.0"
    ]
  },
  "prepare_command": "mkdir -p Pods/SSOLoginClient\nPods/!ProtoCompiler/protoc         --plugin=protoc-gen-grpc=Pods/!ProtoCompiler-gRPCPlugin/grpc_objective_c_plugin         --objc_out=Pods/SSOLoginClient         --grpc_out=Pods/SSOLoginClient         -I ../         -I Pods/!ProtoCompiler         ..//ssologin.proto",
  "pod_target_xcconfig": {
    "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) GPB_USE_PROTOBUF_FRAMEWORK_IMPORTS=1",
    "CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES": "YES"
  },
  "subspecs": [
    {
      "name": "Messages",
      "source_files": [
        "Pods/SSOLoginClient/*.pbobjc.{h,m}",
        "Pods/SSOLoginClient/**/*.pbobjc.{h,m}"
      ],
      "header_mappings_dir": "Pods/SSOLoginClient",
      "requires_arc": false,
      "dependencies": {
        "Protobuf": [

        ]
      }
    },
    {
      "name": "Services",
      "source_files": [
        "Pods/SSOLoginClient/*.pbrpc.{h,m}",
        "Pods/SSOLoginClient/**/*.pbrpc.{h,m}"
      ],
      "header_mappings_dir": "Pods/SSOLoginClient",
      "requires_arc": true,
      "dependencies": {
        "gRPC-ProtoRPC": [

        ],
        "SSOLoginClient/Messages": [

        ]
      }
    }
  ]
}
